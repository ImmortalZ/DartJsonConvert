<idea-plugin>
  <id>immortal.me.dart_json_convert</id>
  <name>DartJsonCovert</name>
  <version>1.0.0</version>
  <vendor email="immortalz.cn@gmail.com" url="https://github.com/ImmortalZ/DartJsonConvert">github</vendor>

  <description><![CDATA[
      step1：select the specify Class <br>
      step2：Code -> Generate -> DartJsonConvert <br>
      step3：generate target code <br>
